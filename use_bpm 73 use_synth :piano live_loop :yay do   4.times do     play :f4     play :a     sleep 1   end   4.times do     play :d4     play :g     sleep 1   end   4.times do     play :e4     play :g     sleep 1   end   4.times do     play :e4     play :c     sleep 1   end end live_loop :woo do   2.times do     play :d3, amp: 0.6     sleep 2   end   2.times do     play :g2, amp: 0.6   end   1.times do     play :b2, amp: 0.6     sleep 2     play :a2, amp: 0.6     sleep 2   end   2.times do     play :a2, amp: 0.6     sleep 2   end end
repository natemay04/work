turbo = "C:/Users/nathan_mayeda/Downloads/turbo.wav"

use_bpm 120

define :notes do |n1, n2|
  play n1
  play n2
  sleep 1
end

partone = ["f4", "a"]
parttwo = ["d4", "g"]

define :piano do
  use_synth :piano
  use_synth_defaults amp: 2
  4.times do
    play (partone)
    sleep 1
  end
  4.times do
    play (parttwo)
    sleep 1
  end
  4.times do
    notes :e4, :g
  end
  4.times do
    notes :e4, :c
  end
end

live_loop :define do
  piano
end

live_loop :woo do
  n = 300
  1.times do
    
    1.times do
      sample :bd_808, amp: n
      sleep 4
    end
    #stop
    n = 150
    5.times do
      sample :bd_808, amp: n + 135
      sleep 4
    end
    #stop
    1000.times do
      sample :bd_808, amp: 825
      sleep 4
    end
  end
  stop
end

sleep 16
live_loop :yolo do
  with_fx :autotuner do
    sample :bd_sone, amp: 2
    sleep 2
  end
end

sleep 8
live_loop :runthatbac do
  1.times do
    sample turbo, amp: 2
    sleep 1
  end
  stop
end
