use_bpm 114



define :piano do
  use_synth :piano
  use_synth_defaults amp: 1.5
  4.times do
    play :f4
    play :a
    sleep 1
  end
  4.times do
    play :d4
    play :g
    sleep 1
  end
  4.times do
    play :e4
    play :g
    sleep 1
  end
  4.times do
    play :e4
    play :c
    sleep 1
  end
end

live_loop :define do
  piano
end


live_loop :woo do
  with_fx :compressor do
    sample :bd_808, amp: 1199
    sleep 4
  end
end

sleep 16
live_loop :yolo do
  sample :bd_sone, amp: 2
  sleep 2
end



